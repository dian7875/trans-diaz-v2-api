{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
      "assets": [
      {
        "include": "reports/template/**/*",
        "outDir": "dist/src"
      },
      {
        "include": "quotation/template/**/*",
        "outDir": "dist/src"
      }
    ]
  }
}
